					"script": {
						"exec": [
							"pm.test(\"Test for data model\", () => {",
							"    const responseJson = pm.response.json();",
							"    pm.expect(responseJson.id).to.eql(1);",
							"    pm.expect(responseJson.petId).to.eql(1);",
							"    pm.expect(responseJson.quantity).to.eql(1);",
							"    pm.expect(responseJson.shipDate).to.eql(\"2022-02-12T11:11:11.000+0000\");",
							"    pm.expect(responseJson.status).to.eql(\"a\")",
							"    pm.expect(responseJson.status).to.be.a('string')",
							"    pm.expect(responseJson.complete).to.eql(true);",
							"    pm.response.to.have.status(200);",
							"    pm.expect(pm.response.responseTime).to.be.below(300);",
							"});",
							""
						],
						"type": "text/javascript"
					}
				}
